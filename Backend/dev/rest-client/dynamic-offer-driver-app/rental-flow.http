@token1 = {{driver-offer-bpp-auto-token1}}

@driverId = favorit-auto1-0000000000000000000000
@driver-offer-bpp-host = http://localhost:8016
@location-tracking-service-host = http://localhost:8081
@merchantId = favorit0-0000-0000-0000-00000favorit

# time in future
@rentalStartTime = 2023-10-30T06:20:15.585221416Z
# @now = $localDatetime iso8601

# @name healthcheck
GET {{driver-offer-bpp-host}}/ui

###
# @name healthcheck
GET {{app-host}}
###

# @name setActive
POST {{driver-offer-bpp-host}}/ui/driver/setActivity?active=true&mode="ONLINE"
content-type: application/json
token: {{token1}}

###
@adminToken = favorit-admin-0000000000000000-token

# @name updateVehicle

POST {{driver-offer-bpp-host}}/ui/org/vehicle/{{driverId}}
content-type: application/json
token: {{adminToken}}

{
    "variant" : "SEDAN"
}

###
# @name updateInitial
POST {{location-tracking-service-host}}/ui/driver/location
token: {{token1}}
mId: {{merchantId}}
vt: SEDAN
dm: ONLINE
content-type: application/json

[
    {
        "pt": {
            "lat": 14.445332,
            "lon": 75.919028

        },
        "ts": "{{$localDatetime iso8601}}",
        "acc": 0
    }
]

###

# @name enableRental
POST {{driver-offer-bpp-host}}/ui/driver/enableRental?flag=true
content-type: application/json
token: {{token1}}

###

# @name search
POST {{app-host}}/rideSearch
content-type: application/json
token: {{app-token}}

{
  "fareProductType" : "RENTAL",
  "contents" : {
    "origin": {
      "address": {
          "street": "60 Feet Rd",
          "state": "Karnataka",
          "door": "",
          "country": "India",
          "city": "Davanagere",
          "building": " ",
          "areaCode": "",
          "area": "Vidyanagar"
      },
      "gps": {
          "lat": 14.445332,
          "lon": 75.919028
      }
    },
    "startTime" : "{{rentalStartTime}}"
  }
}

###

@searchId = {{search.response.body.searchId}}

# @name quotes
GET {{app-host}}/rideSearch/{{searchId}}/results
content-type: application/json
token: {{app-token}}

###
@selectedQuoteId = {{quotes.response.body.quotes[0].onRentalCab.id}}

# @name confirmQuote

# POST {{app-host}}/rideSearch/quotes/{{selectedQuoteId}}/confirm?paymentMethodId={{paymentMethodId}}

POST {{app-host}}/rideSearch/quotes/{{selectedQuoteId}}/confirm?startTime={{rentalStartTime}}&rentalDuration=2
content-type: application/json
token: {{app-token}}

###

# Should be updated just before AllocateRentalRide job triggered:
# job.scheduledAt = max booking.createdAt (rentalStartTime - 15 mins)

